{
	"manifest_version": 3,
	"name": "Chim_Utility",
	"description": "침하하를 더 편하게 이용하고 싶어!",
	"version": "1.1",
	"icons": {
		"128" : "img/128.png"
	},
	"action": {
		"default_icon" : "img/128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://chimhaha.net/",
				"https://chimhaha.net/*"
			],
			"exclude_matches": [
				"https://chimhaha.net/*/new",
				"https://chimhaha.net/*/edit",
				"https://chimhaha.net/mypage/*",
				"https://chimhaha.net/alarm",
				"https://chimhaha.net/login",
				"https://chimhaha.net/join"
			],
			"js": ["movePage.js"]
		}
	]
}